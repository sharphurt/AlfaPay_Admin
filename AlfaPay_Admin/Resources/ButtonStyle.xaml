    <Style x:Key="OutlinedWhiteButtonStyle" TargetType="Button">
        <Style.Resources>
            <Color x:Key="PrimaryBackgroundColor" R="255" G="255" B="255" A="255" />
            <Color x:Key="PrimaryBorderColor" R="157" G="165" B="174" A="255" />
            <Color x:Key="MouseOverBackgroundColor" R="255" G="255" B="255" A="255" />
